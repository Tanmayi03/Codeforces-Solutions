#include <iostream>
#include <string>
using namespace std;
 
int main()
{
    string t;
    cin>>t;
    int c = 5;
    int k=0;
    while(c>0)
    {
        string h;
        cin>>h;
        if((t[0]==h[0])||(t[1]==h[1]))
        {
            k++;
        }
        c--;
    }
    if(k>0)
    {
        cout<<"YES"<<endl;
    }
    else
    {
        cout<<"NO"<<endl;
    }
    return 0;
}
